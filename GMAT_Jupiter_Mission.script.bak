%----------------------------------------
%---------- Mission Sequence
%----------------------------------------

BeginMissionSequence;

If 'Instant analysis' CASE == 1
   Target 'TargetJupiter' DC1 {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
      
      Vary 'VaryVx' DC1(HI.Element1 = 8.920298847316754, {Perturbation = 0.0001, Lower = 8.5, Upper = 9.5, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVy' DC1(HI.Element2 = 0.08314860547934715, {Perturbation = 0.0001, Lower = -0.1, Upper = 0.1, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVz' DC1(HI.Element3 = 0.1, {Perturbation = 0.0001, Lower = -0.1, Upper = 0.1, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Maneuver 'HM' HI(DALA_Sat);
      
      BeginScript 'ExtractVinf_E'
         GMAT VinfX_E = DALA_Sat.EarthMJ2000Eq.VX;
         GMAT VinfY_E = DALA_Sat.EarthMJ2000Eq.VY;
         GMAT VinfZ_E = DALA_Sat.EarthMJ2000Eq.VZ;
      EndScript;
      Propagate 'PropToJupiter' DeepSpaceProp(DALA_Sat) {DALA_Sat.ElapsedDays = TOF, OrbitColor = [255 0 0]};
      Achieve 'ReachJupiter' DC1(DALA_Sat.Jupiter.RMAG = 0, {Tolerance = JupiterTOL});
   EndTarget;  % For targeter DC1
   
   BeginScript 'DefineSecondSat'
      GMAT C3_E = VinfX_E^2+VinfY_E^2+VinfZ_E^2; % m^2/s^2
      GMAT alpha_E = RadToDeg(atan2(VinfY_E, VinfX_E)); % Grados
      GMAT delta_E = RadToDeg(asin(VinfZ_E/sqrt(C3_E))); % Grados
      GMAT phi_E = (asin(sin(DegToRad(delta_E))/sin(DegToRad(INC_inicial)))); % Radianes
      
      %GMAT BVAZI_E = 90 + RadToDeg(sin(phi_E+DegToRad(90))*sin(DegToRad(INC_inicial))/sin(DegToRad(delta_E+90)));
      GMAT BVAZI_E = 90 + RadToDeg(asin((cos(phi_E)*sin(DegToRad(INC_inicial)))/cos(DegToRad(delta_E)))); % Grados
      
      GMAT DALA_SatFine.EarthMJ2000Eq.OutgoingRHA = alpha_E; % Grados
      GMAT DALA_SatFine.EarthMJ2000Eq.OutgoingDHA = delta_E; % Grados
      GMAT DALA_SatFine.Earth.OutgoingC3Energy = C3_E; % m^2/s^2
      GMAT DALA_SatFine.EarthMJ2000Eq.OutgoingBVAZI = BVAZI_E; % Grados
      
      GMAT RAAN_E = DALA_SatFine.EarthMJ2000Eq.RAAN;
      GMAT AOP_E = DALA_SatFine.EarthMJ2000Eq.AOP;
   EndScript;
   
   BeginScript 'getDV_HM'
      GMAT dV_HM = sqrt(398600.4415*(2/7300-1/DALA_SatFine.Earth.SMA))-sqrt(398600.4415/7300);
   EndScript;
   
   UpdateDynamicData 'UpdateTable' TABLE;
   Propagate 'PropToEarthSOI' NearEarth(DALA_SatFine) {DALA_SatFine.Earth.RMAG = 0.924e6, OrbitColor = [255 128 0]};
   GMAT 'TOF MCM_tan' TOF_MCM_tan = TOF/3;
   GMAT 'TOF MCM_inc' TOF_MCM_inc = TOF/3;
   Propagate 'PropToMCM_tan' DeepSpaceProp(DALA_SatFine) {DALA_SatFine.ElapsedDays = TOF_MCM_tan, OrbitColor = [255 255 0]};
   
   
   Target 'TargetIncomingOrbit' DC1 {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
      
      Vary 'VaryVx' DC1(MCI_tan.Element1 = -0.3080007948750442, {Perturbation = 0.001, Lower = -3, Upper = 3, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVy' DC1(MCI_tan.Element2 = 0.05535358237452325, {Perturbation = 0.001, Lower = -3, Upper = 3, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVz' DC1(MCI_inc.Element3 = -0.006740414992889346, {Perturbation = 0.001, Lower = -3, Upper = 3, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Maneuver 'MCM_tan' MCI_tan(DALA_SatFine);
      Propagate 'PropToMCM_inc' DeepSpaceProp(DALA_SatFine) {DALA_SatFine.Jupiter.RMAG = 10e6, DALA_SatFine.ElapsedDays = TOF_MCM_inc, OrbitColor = [255 0 0]};
      Maneuver 'MCM_inc' MCI_inc(DALA_SatFine);
      Propagate 'PropToJupiterSOI' DeepSpaceProp(DALA_SatFine) {DALA_SatFine.Jupiter.RMAG = 48.2e6, DALA_SatFine.ElapsedDays = 365, OrbitColor = [255 128 0]};
      
      BeginScript 'Rp and BVAZI'
         %GMAT VinfX_J = DALA_SatFine.JupiterEq.VX;
         %GMAT VinfY_J = DALA_SatFine.JupiterEq.VY;
         %GMAT VinfZ_J = DALA_SatFine.JupiterEq.VZ;
         
         %GMAT Vinf_J = sqrt(VinfX_J^2+VinfY_J^2+VinfZ_J^2);
         
         
         %GMAT delta_Jup = RadToDeg(asin(VinfZ_J/Vinf_J));
         %GMAT phi_Jup = asin(sin(DegToRad(delta_Jup))/sin(DegToRad(INC_final)));
         %GMAT BVAZI_Jup = 90 + RadToDeg(asin(cos(phi_Jup+DegToRad(90))*sin(DegToRad(INC_final))/sin(DegToRad(delta_Jup+90))));
         
         GMAT Rp_Jup = Hp_Jup + 71492;
         
      EndScript;
      Propagate 'PropToPeriapsis' NearJupiter(DALA_SatFine) {DALA_SatFine.Jupiter.Periapsis, DALA_SatFine.ElapsedDays = 365, OrbitColor = [255 255 0]};
      Achieve 'AchieveHp' DC1(DALA_SatFine.Jupiter.RadPer = Rp_Jup, {Tolerance = 10});
      Achieve 'AchieveINC' DC1(DALA_SatFine.JupiterEq.INC = INC_final, {Tolerance = 0.1});
   EndTarget;  % For targeter DC1
   BeginScript 'getDV_MCM'
      GMAT dV_MCM = sqrt(MCI_tan.Element1^2+MCI_tan.Element3^2)+sqrt(MCI_inc.Element2^2);
   EndScript;
   UpdateDynamicData 'UpdateTable2' TABLE;
   If 'Fly-by' ECC >= 1.0
      Propagate 'PropFly-by' NearJupiter(DALA_SatFine) {DALA_SatFine.Jupiter.RMAG = 48.2e6, OrbitColor = [255 0 0]};
   EndIf;
   If 'Jup_orbit' ECC < 1
      
      Target 'TargetOrbit' DC1 {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
         Vary 'VaryVx' DC1(FI.Element1 = -6.740115756797051, {Perturbation = 0.0001, Lower = -15, Upper = 15, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         Vary 'VaryVy' DC1(FI.Element2 = -2.810613697123259, {Perturbation = 0.0001, Lower = -15, Upper = 15, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         Vary 'VaryVz' DC1(FI.Element3 = -0.1577853021651958, {Perturbation = 0.0001, Lower = -3, Upper = 3, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         Maneuver 'FM' FI(DALA_SatFine);
         Propagate 'PropInOrbit' NearJupiter(DALA_SatFine) {DALA_SatFine.ElapsedSecs = 1200, OrbitColor = [255 0 0]};
         Achieve 'AchieveHp' DC1(DALA_SatFine.Jupiter.RadPer = Rp_Jup, {Tolerance = 50});
         Achieve 'AchieveINC' DC1(DALA_SatFine.JupiterEq.INC = INC_final, {Tolerance = 3});
         Achieve 'AchieveECC' DC1(DALA_SatFine.Jupiter.ECC = ECC, {Tolerance = 0.01});
      EndTarget;  % For targeter DC1
      BeginScript 'getDV_FM'
         GMAT dV_FM = sqrt(FI.Element1^2+FI.Element2^2+FI.Element3^2);
      EndScript;
      UpdateDynamicData 'UpdateTable3' TABLE;
      Propagate 'PropInJup' NearJupiter(DALA_SatFine) {DALA_SatFine.ElapsedDays = 2, OrbitColor = [255 128 64]};
   EndIf;
   BeginScript 'getDV'
      GMAT dV_TOT = abs(dV_HM) + abs(dV_MCM) + abs(dV_FM);
      GMAT Mf_MCM = Mf_FM * exp(dV_FM*1000/(Isp_inj*9.81));
      GMAT Mf_HM = Mf_MCM * exp(dV_MCM*1000/(Isp_corr*9.81));
      GMAT M0 = Mf_HM * exp(dV_HM*1000/(Isp_inj*9.81));
      GMAT Mp = M0 - Mf_FM;
      GMAT Mp_HI = M0 - Mf_HM;
      GMAT Mp_MCI = Mf_HM - Mf_MCM;
      GMAT Mp_FI = Mf_MCM - Mf_FM;
   EndScript;
   UpdateDynamicData 'UpdateTable4' TABLE;
EndIf;
If 'Finite analysis' CASE == 1
   
   BeginScript 'DefineThirdSat'
      GMAT DALA_SatFinite.EarthMJ2000Eq.RAAN = RAAN_E;
      GMAT DALA_SatFinite.EarthMJ2000Eq.AOP = AOP_E;
      
      GMAT DALA_SatFinite.DALA_Tank_Finite.FuelMass = Mp*1.1;
   EndScript;
   Propagate 'PropParking' NearEarth(DALA_SatFinite) {DALA_SatFinite.Earth.Periapsis, OrbitColor = [0 255 255]};
   Target 'TargetOutgoingH' DC1 {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
      Vary 'VaryVx' DC1(DALA_SatFinite.Thruster_H.ThrustDirection1 = 0.9987413786880269, {Perturbation = 0.0001, Lower = -1, Upper = 1, MaxStep = 0.05, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVy' DC1(DALA_SatFinite.Thruster_H.ThrustDirection2 = -0.006430418701263884, {Perturbation = 0.0001, Lower = -1, Upper = 1, MaxStep = 0.01, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVz' DC1(DALA_SatFinite.Thruster_H.ThrustDirection3 = 0.1409241528589643, {Perturbation = 0.0001, Lower = -1, Upper = 1, MaxStep = 0.1, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'Vary_tb_HM' DC1(tb_HM = 207.580196259775, {Perturbation = 0.001, Lower = 0, Upper = 500, MaxStep = 10, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      BeginFiniteBurn 'Begin_HM' HF(DALA_SatFinite);
      Propagate 'PropBurn' NearEarth(DALA_SatFinite) {DALA_SatFinite.ElapsedSecs = tb_HM, OrbitColor = [0 255 255]};
      EndFiniteBurn 'End_HM' HF(DALA_SatFinite);
      Achieve 'AchieveAlpha' DC1(DALA_SatFinite.EarthMJ2000Eq.OutgoingRHA = alpha_E, {Tolerance = 1e-3});
      Achieve 'AchieveDelta' DC1(DALA_SatFinite.EarthMJ2000Eq.OutgoingDHA = delta_E, {Tolerance = 1e-3});
      Achieve 'AchieveC3_E' DC1(DALA_SatFinite.Earth.OutgoingC3Energy = C3_E, {Tolerance = 0.2});
   EndTarget;  % For targeter DC1
   BeginScript 'getMp_HF'
      GMAT Mp_HF = E_inj/Isp_inj * tb_HM;
   EndScript;
   UpdateDynamicData 'UpdateTable5' TABLE_FINITE;
   Propagate 'PropToOutgoingH' NearEarth(DALA_SatFinite) {DALA_SatFinite.Earth.RMAG = 0.924e6, OrbitColor = [255 128 255]};
   Propagate 'PropToCorr_tan' DeepSpaceProp(DALA_SatFinite) {DALA_SatFinite.ElapsedDays = TOF_MCM_tan, OrbitColor = [0 255 255]};
   
   Target 'TargetIncomingOrbit' DC1 {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
      
      Vary 'VaryVx' DC1(DALA_SatFinite.Thruster_MC_tan.ThrustDirection1 = -1, {Perturbation = 0.001, Lower = -1, Upper = 1, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVy' DC1(DALA_SatFinite.Thruster_MC_tan.ThrustDirection2 = -1, {Perturbation = 0.001, Lower = -1, Upper = 1, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'VaryVz' DC1(DALA_SatFinite.Thruster_MC_inc.ThrustDirection3 = 0.451, {Perturbation = 0.001, Lower = -1, Upper = 1, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'Vary_tb_MCM_tan' DC1(tb_MCM_tan = 28797.472700639, {Perturbation = 0.0001, Lower = 1e3, Upper = 1e6, MaxStep = 1e3, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      Vary 'Vary_tb_MCM_inc' DC1(tb_MCM_inc = 0, {Perturbation = 0.0001, Lower = 0, Upper = 1e6, MaxStep = 1e3, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
      BeginFiniteBurn 'Begin_MCM_tan' MCF_tan(DALA_SatFinite);
      Propagate 'PropBurn' DeepSpaceProp(DALA_SatFinite) {DALA_SatFinite.ElapsedSecs = tb_MCM_tan, OrbitColor = [0 0 255]};
      EndFiniteBurn 'End_MCM_tan' MCF_tan(DALA_SatFinite);
      Propagate 'PropToMCM_inc' DeepSpaceProp(DALA_SatFinite) {DALA_SatFinite.Jupiter.RMAG = 10e6, DALA_SatFinite.ElapsedDays = TOF_MCM_inc, OrbitColor = [255 0 255]};
      BeginFiniteBurn 'Begin_MCM_inc' MCF_inc(DALA_SatFinite);
      Propagate 'PropBurn' DeepSpaceProp(DALA_SatFinite) {DALA_SatFinite.ElapsedSecs = tb_MCM_inc, OrbitColor = [0 255 255]};
      EndFiniteBurn 'End_MCM_inc' MCF_inc(DALA_SatFinite);
      Propagate 'PropToJupiterSOI' DeepSpaceProp(DALA_SatFinite) {DALA_SatFinite.Jupiter.RMAG = 48.2e6, DALA_SatFinite.ElapsedDays = 365, OrbitColor = [0 0 255]};
      
      BeginScript 'Rp and BVAZI'
         GMAT Rp_Jup = Hp_Jup + 71492;
      EndScript;
      Propagate 'PropToPeriapsis' NearJupiter(DALA_SatFinite) {DALA_SatFinite.Jupiter.Periapsis, DALA_SatFinite.ElapsedDays = 365, OrbitColor = [255 0 255]};
      Achieve 'AchieveHp' DC1(DALA_SatFinite.Jupiter.RadPer = Rp_Jup, {Tolerance = 1000});
      Achieve 'AchieveINC' DC1(DALA_SatFinite.JupiterEq.INC = INC_final, {Tolerance = 150});
   EndTarget;  % For targeter DC1
   BeginScript 'getMp_MCF'
      GMAT Mp_MCF_tan = E_corr/Isp_corr * tb_MCM_tan;
      GMAT Mp_MCF_inc = E_corr/Isp_corr * tb_MCM_inc;
   EndScript;
   UpdateDynamicData 'UpdateTable6' TABLE_FINITE;
   
   If 'Fly-by' ECC >= 1.0
      Propagate 'PropFly-by' NearJupiter(DALA_SatFinite) {DALA_SatFinite.Jupiter.RMAG = 48.2e6, OrbitColor = [0 255 255]};
   EndIf;
   If 'Jup_orbit' ECC < 1
      
      Target 'TargetOrbit' DC1 {SolveMode = Solve, ExitMode = DiscardAndContinue, ShowProgressWindow = true};
         Vary 'VaryVx' DC1(DALA_SatFinite.Thruster_F.ThrustDirection1 = 1, {Perturbation = 0.0001, Lower = -10, Upper = 10, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         Vary 'VaryVy' DC1(DALA_SatFinite.Thruster_F.ThrustDirection2 = 0, {Perturbation = 0.0001, Lower = -10, Upper = 10, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         Vary 'VaryVz' DC1(DALA_SatFinite.Thruster_F.ThrustDirection3 = 0, {Perturbation = 0.0001, Lower = -3, Upper = 3, MaxStep = maxStep, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         Vary 'Vary_tb_FM' DC1(tb_FM = 2, {Perturbation = 0.0001, Lower = 0.0, Upper = 100, MaxStep = 0.2, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
         BeginFiniteBurn 'Begin_FM' FF(DALA_SatFinite);
         Propagate 'PropBurn' NearJupiter(DALA_SatFinite) {DALA_SatFinite.ElapsedSecs = tb_FM, OrbitColor = [255 0 255]};
         EndFiniteBurn 'End_FM' FF(DALA_SatFinite);
         Propagate 'PropInOrbit' NearJupiter(DALA_SatFinite) {DALA_SatFinite.ElapsedSecs = 1200, OrbitColor = [0 0 255]};
         Achieve 'AchieveHp' DC1(DALA_SatFinite.Jupiter.RadPer = Rp_Jup, {Tolerance = 50});
         Achieve 'AchieveINC' DC1(DALA_SatFinite.JupiterEq.INC = INC_final, {Tolerance = 3});
         Achieve 'AchieveECC' DC1(DALA_SatFinite.Jupiter.ECC = ECC, {Tolerance = 0.01});
      EndTarget;  % For targeter DC1
      Propagate 'PropInJup' NearJupiter(DALA_SatFinite) {DALA_SatFinite.ElapsedDays = 2, OrbitColor = [255 0 255]};
      BeginScript 'getMp_FM'
         GMAT Mp_FF = E_inj/Isp_inj * tb_FM;
      EndScript;
      UpdateDynamicData 'UpdateTable7' TABLE_FINITE;
   EndIf;
   BeginScript 'getMp_F'
      GMAT Mp_F = Mp_HF + Mp_MCF_tan + Mp_MCF_inc + Mp_FF;
   EndScript;
   UpdateDynamicData 'UpdateTable8' TABLE_FINITE;





EndIf;
